{
  "expo": {
    "name": "NumBlitz",
    "slug": "numblitz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "numblitz",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff5f6"
    },
    "ios": {
      "bundleIdentifier": "com.shanthatos.numblitz",
      "supportsTablet": true,
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#fff5f6"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/katex/Katex.ttf",
            "./assets/fonts/katex/Katex_bold.ttf",
            "./assets/fonts/katex/Katex_italic.ttf",
            "./assets/fonts/katex/Katex_bold_italic.ttf"
          ]
        }
      ],
      "expo-secure-store",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2cf7091b-2b81-4049-8b16-92d24861a669"
      }
    }
  }
}
